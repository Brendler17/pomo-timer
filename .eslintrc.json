{
  "extends": [
    "airbnb",
    "eslint-config-airbnb-typescript"
  ],
  "parserOptions": {
    "project": [
      "./tsconfig.json",
      "./tsconfig.node.json"
    ]
  },
  "rules": {
    "import/prefer-default-export": "off",
    "react/react-in-jsx-scope": "off",
    "react/jsx-uses-react": "off",
    "@typescript-eslint/no-empty-interface": [
      "error",
      {
        "allowSingleExtends": true
      }
    ]
  }
}